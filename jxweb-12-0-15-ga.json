{
  "id": "jxweb-12-0-15-ga",
  "name": "CA Directory 12.0.15.ga LDAP Browser",
  "replicationControllerList": {
    "apiVersion": "v1beta3",
    "items": [
      {
        "metadata": {
          "annotations": {
            "description": "JXWeb Replication Controller"
          },
          "labels": {
            "ca.com/jxweb-12-0-15-ga": "web"
          },
          "name": "jxweb-rc",
          "namespace": "utility"
        },
        "spec": {
          "replicas": 1,
          "selector": {
            "ca.com/jxweb-12-0-15-ga": "web"
          },
          "template": {
            "metadata": {
              "annotations": {
                "description": "JXWeb Pod"
              },
              "labels": {
                "ca.com/jxweb-12-0-15-ga": "web"
              },
              "name": "web",
              "namespace": "utility"
            },
            "spec": {
              "containers": [
                {
                  "image": "dev-store.vapp-security.solutions/directory/jxweb:12.0.15.ga",
                  "livenessProbe": {
                    "httpGet": {
                      "path": "/",
                      "port": "9090"
                    },
                    "initialDelaySeconds": 30
                  },
                  "name": "web",
                  "ports": [
                    {
                      "containerPort": 9090
                    }
                  ]
                }
              ],
              "dnsPolicy": "ClusterFirst",
              "restartPolicy": "Always"
            }
          }
        }
      }
    ]
  },
  "serviceList": {
    "items": [
      {
        "apiVersion": "v1beta3",
        "metadata": {
          "annotations": {
            "ca.com/cluster-public-urlpath": "/jxweb",
            "ca.com/cluster-public-urlpath-backend": "/jxweb",
            "description": "Public service to expose JXWeb UI"
          },
          "labels": {
            "ca.com/cluster-public-service": "true"
          },
          "name": "jxweb",
          "namespace": "utility"
        },
        "spec": {
          "ports": [
            {
              "port": 80,
              "targetPort": 9090
            }
          ],
          "selector": {
            "ca.com/jxweb-12-0-15-ga": "web"
          }
        }
      }
    ]
  }
}
