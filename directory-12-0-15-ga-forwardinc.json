{
  "id": "dir-12-0-15-ga-forwardinc",
  "name": "CA Directory 12.0.15.ga ForwardInc",
  "serviceList": {
    "items": [
      {
        "apiVersion": "v1beta3",
        "metadata": {
          "name": "ldap",
          "namespace": "dir-12-0-15-ga",
          "annotations": {
            "description": "Public service to CA Directory",
            "ca.com/cluster-public-url": "/jxweb/ConnectPageServlet?host=ldap.dir-12-0-15-ga.cluster.local&port=389&baseDN=dc=forwardinc,dc=ca"
          },
          "labels": {
            "ca.com/cluster-public-service": "tcp",
            "ca.com/dir-12-0-15-ga": "2015-04-21"
          }
        },
        "spec": {
          "ports": [
            {
              "port": 389,
              "targetPort": 1389
            }
          ],
          "sessionAffinity": "ClientIP",
          "selector": {
            "ca.com/dir-12-0-15-ga": "2015-04-21"
          }
        }
      },
      {
        "apiVersion": "v1beta3",
        "metadata": {
          "name": "dxgrid-discovery",
          "namespace": "dir-12-0-15-ga",
          "annotations": {
            "description": "Private service to CA Directory discovery"
          },
          "labels": {
            "ca.com/dir-12-0-15-ga": "2015-04-21"
          }
        },
        "spec": {
          "ports": [
            {
              "port": 2379,
              "targetPort": 2379
            }
          ],
          "selector": {
            "ca.com/dir-12-0-15-ga": "2015-04-21",
            "discovery": "bootstrap"
          }
        }
      }
    ]
  },
  "replicationControllerList": {
    "items": [
      {
        "apiVersion": "v1beta3",
        "metadata": {
          "name": "dxgrid-primary-rc",
          "namespace": "dir-12-0-15-ga",
          "labels": {
            "ca.com/dir-12-0-15-ga": "2015-04-21"
          },
          "annotations": {
            "description": "dxgrid primary Replication Controller"
          }
        },
        "spec": {
          "replicas": 1,
          "selector": {
            "ca.com/dir-12-0-15-ga": "2015-04-21",
            "dxgrid": "primary"
          },
          "template": {
            "metadata": {
              "name": "dxgrid-primary",
              "namespace": "dir-12-0-15-ga",
              "labels": {
                "ca.com/dir-12-0-15-ga": "2015-04-21",
                "dxgrid": "primary"
              },
              "annotations": {
                "description": "dxgrid primary Pod"
              }
            },
            "spec": {
              "restartPolicy": "Always",
              "dnsPolicy": "ClusterFirst",
              "volumes": [
                {
                  "name": "dxgrid-configsvc",
                  "source": {
                    "emptyDir": {}
                  }
                }
              ],
              "containers": [
                {
                  "name": "dxgrid-router",
                  "image": "store.vapp-security.solutions/directory/dxgrid:12.0.15.ga",
                  "env": [
                    {
                      "name": "CONFIG",
                      "value": "dxgrid-forwardinc/primary-router"
                    }
                  ],
                  "ports": [
                    {
                      "containerPort": 1389
                    }
                  ],
                  "volumeMounts": [
                    {
                      "name": "dxgrid-configsvc",
                      "mountPath": "/solution"
                    }
                  ]
                },
                {
                  "name": "dxgrid-data",
                  "image": "store.vapp-security.solutions/directory/dxgrid:12.0.15.ga",
                  "env": [
                    {
                      "name": "CONFIG",
                      "value": "dxgrid-forwardinc/primary-data"
                    }
                  ],
                  "ports": [
                    {
                      "containerPort": 2389
                    }
                  ],
                  "volumeMounts": [
                    {
                      "name": "dxgrid-configsvc",
                      "mountPath": "/solution"
                    }
                  ]
                },
                {
                  "name": "dxgrid-configsvc",
                  "image": "store.vapp-security.solutions/vapp/sidecar-configsvc:latest",
                  "env": [
                    {
                      "name": "SOURCE",
                      "value": "https://github.com/ca-directory/vapp-config-forwardinc/archive/12.0.15.ga.zip"
                    },
                    {
                      "name": "DESTINATION",
                      "value": "/solution"
                    }
                  ],
                  "volumeMounts": [
                    {
                      "name": "dxgrid-configsvc",
                      "mountPath": "/solution"
                    }
                  ]
                }
              ]
            }
          }
